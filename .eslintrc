{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "env": {
	"browser": true,
	"commonjs": true,
	"node": true,
	"es6": true
  },
  "parserOptions": {
	"ecmaVersion": 6
  },
  "rules": {
	"no-console": "off",
	"strict": ["error", "global"],
	"curly": "warn",
	"no-unused-vars": [2, { "vars": "all", "args": "none" }]
  }
}

